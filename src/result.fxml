<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBox" alignment="center" prefHeight="416.0" prefWidth="386.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ResultController">
    <Label fx:id="titleLabel" text="Student Result" />
    <TableView fx:id="resultTable" onSort="#initialize">
        <columns>
            <TableColumn fx:id="subjectColumn" prefWidth="234.0" text="Subject" />
         <TableColumn fx:id="codeColumn" minWidth="0.0" prefWidth="85.0" text="Code" />
            <TableColumn fx:id="gradeColumn" prefWidth="66.0" text="Grade" />
        </columns>
    </TableView>
</VBox>
